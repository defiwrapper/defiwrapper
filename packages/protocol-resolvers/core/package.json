{
  "name": "@defiwrapper/core-protococol-resolver",
  "description": "core protocol resolver",
  "private": true,
  "version": "0.0.1-prealpha.61",
  "scripts": {
    "lint": "eslint . --ext .ts --ignore-path ../../.eslintignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path ../../.eslintignore",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "build": "npx w3 build",
    "codegen": "npx w3 codegen",
    "watch": "npx w3 build -i -e core.protocol-resolver.defiwrapper.eth -w",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "npx w3 build --ipfs http://localhost:5001 --test-ens core.protocol-resolver.defiwrapper.eth",
    "recipes": "ts-node ./recipes/index.ts",
    "test": "yarn test:e2e",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@web3api/cli": "0.0.1-prealpha.61",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.61",
    "@web3api/test-env-js": "^0.0.1-prealpha.61",
    "@web3api/wasm-as": "0.0.1-prealpha.61",
    "as-big": "^0.1.5",
    "assemblyscript": "0.19.1",
    "eslint": "^7.32.0",
    "ethers": "5.0.7",
    "jest": "^27.4.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
